{
  "name": "gilton-system",
  "version": "1.0.0",
  "description": "Real-time worship support system with Electron server and React PWA client",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:electron\"",
    "dev:client": "vite",
    "dev:electron": "tsc -p tsconfig.electron.json && electron dist/electron/main.js",
    "build": "npm run build:client && npm run build:electron",
    "build:client": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "start": "electron dist/electron/main.js",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx .",
    "typecheck": "tsc -p tsconfig.client.json --noEmit && tsc -p tsconfig.electron.json --noEmit",
    "test": "npm run lint && npm run typecheck"
  },
  "keywords": [
    "worship",
    "church",
    "electron",
    "react",
    "pwa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.8.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.2",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.0.0-beta.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "electron-vite": "^1.0.28",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.7"
  }
}
